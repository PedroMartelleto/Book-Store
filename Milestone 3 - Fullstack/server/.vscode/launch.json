    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "env": {
                "MONGO_URL": "mongodb://localhost:27017",
                "PORT": "8080",
                "PWD_SECRET_KEY": "secret",
                "JWT_SECRET_KEY": "secret"
            }
        }
    ]
}